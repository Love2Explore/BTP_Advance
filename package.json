{
  "name": "vechile_sidebyside",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^3.15.0",
    "@sap-cloud-sdk/resilience": "^3.15.0",
    "@sap/cds": "^7",
    "@sap/hana-client": "^2.15.17",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "@sap/xssec": "^3",
    "cds-swagger-ui-express": "^0.8.0",
    "cors": "^2.8.5",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7",
    "@sap/eslint-plugin-cds": "^3",
    "eslint": "^9"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "db": "hana",
      "messaging": {
        "kind": "enterprise-messaging"
      },
      "vechile": {
        "kind": "odata-v2",
        "model": "srv/external/vechile",
        "csrf": true,
        "csrfInBatch": true,
        "credentials": {
          "destination": "ERXCLNT001",
          "path": "/sap/opu/odata/sap/UI_VECHILE_INFO_RT"
        }
      },
      "wcm": {
        "kind": "odata",
        "model": "srv/external/wcm",
        "csrf": true,
        "csrfInBatch": true,
        "credentials": {
          "destination": "ERXCLNT001",
          "path": "/sap/opu/odata4/sap/testsbv4/srvd/sap/testsd/0001"
        }
      }
    }
  }
}
